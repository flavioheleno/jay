{
  "name": "flavioheleno/jay",
  "description": "Thin wrapper for json & simdjson",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "simdjson",
    "json"
  ],
  "authors": [
    {
      "name": "Flavio Heleno",
      "email": "flaviohbatista@gmail.com",
      "homepage": "https://flavioheleno.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Jay\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jay\\Test\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist",
    "allow-plugins": {
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^10.3",
    "psy/psysh": "^0.11.20",
    "roave/security-advisories": "dev-latest"
  }
}
